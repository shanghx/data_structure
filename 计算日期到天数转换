#include<iostream>
using namespace std;
int mon[13] = { 0, 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 };
class Data
{
public:
	Data()
	{

	}

	int iConverDateToDay(int year, int month, int day)
	{
		/* 在这里实现功能，将结果填入输入数组中*/
		if (year <= 0 || month <= 0 || day <= 0 || month > 12 || day > 31)
			return -1;
		_year = year;
		_month= month;
		_day = day;
		return 0;
	}



	int getOutDay()
	{
		if ((_year % 4 == 0 && _year % 100 != 0) || _year % 4 == 0)
			mon[2] += 1;
		int res = _day;
		for (int i = 0; i < _month; i++)
		{
			res+=mon[i];
			
		}
		mon[2] =28;
		return res;
	}
private:
	int _year;
	int _month;
	
	int _day;
	
};

int main()
{
	Data d;
    int year,month,day;
   while(cin>>year>>month>>day)
    {
         d.iConverDateToDay(year, month, day);
		//return 0;
	    cout<<d.getOutDay()<<endl;
    }
    return 0;

}
